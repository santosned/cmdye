{
  "name": "cmdye",
  "version": "0.1.0",
  "description": "A tiny tool to colorify console messages.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "@types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prelint": "prettier --write .",
    "lint": "eslint . --ext .ts --fix",
    "dev": "ts-node package/cmdye.ts",
    "types": "tsc --out out/index.js",
    "build": "rollup --config --bundleConfigAsCjs",
    "prepack": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santosned/cmdye.git"
  },
  "keywords": [
    "ansi",
    "color",
    "colors",
    "colours",
    "console",
    "terminal",
    "style",
    "string",
    "cli"
  ],
  "author": "E. M. Santos <santosned@proton.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/santosned/cmdye/issues"
  },
  "homepage": "https://github.com/santosned/cmdye#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "rollup": "^3.15.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
